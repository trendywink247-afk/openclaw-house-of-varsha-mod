{
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token"
    },
    "controlUi": {
      "allowInsecureAuth": true
    }
  },
  "agents": {
    "autoload": true
  },
  "skills": {
    "allowBundled": ["github", "coding-agent", "canvas", "summarize", "telegram", "self-healing-agent"]
  },
  "channels": {
    "telegram": {
      "kind": "telegram",
      "configFile": "telegram-channel-config.json"
    }
  }
}
